{
    "manifest_version": 3,
    "name": "Morten Text Replacement",
    "version": "1.0",
    "description": "Morten Text Replacement prosjekt",
    "content_scripts": [{
        "matches": ["<all_urls>"],
        "js": ["script.js"],
        "css": ["styles.css"]
    }],
    "icons": {
        "16": "icons/logo_16.png",
        "48": "icons/logo_48.png",
        "128": "icons/logo_128.png"
    },
    "web_accessible_resources": [{
        "resources": ["/icons/*.png"],
        "matches": ["<all_urls>"]
    }]
}